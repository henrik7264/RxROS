<?xml version="1.0"?>

<robot name="ros_robot">
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="darkgray">
    <color rgba="0.3 0.3 0.3 1"/>
  </material>
  <material name="lightgray">
    <color rgba="0.8 0.8 0.8 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  
  <link name="base_footprint"/>

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.25 0.14 0.10"/>
      </geometry>
      <material name="lightgray"/>
    </visual>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.0779" rpy="0 0 0"/>
  </joint>

  <link name="laser_frame">
    <visual>
      <geometry>
        <cylinder length="0.041" radius="0.038"/>
      </geometry>
      <material name="darkgray"/>
    </visual>
  </link>

  <joint name="laser_joint" type="fixed">
    <parent link="base_link"/>
    <child link="laser_frame"/>
    <origin xyz="0 0 0.07" rpy="0 0 0"/>
  </joint>

  <link name="r_wheel_frame">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.0279"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

  <joint name="r_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="r_wheel_frame"/>
    <origin xyz="0 -0.07 -0.0578" rpy="1.5707963268 0 0" />
    <axis xyz="0 0 1" />
  </joint>

  <link name="l_wheel_frame">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.0279"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

  <joint name="l_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="l_wheel_frame"/>
    <origin xyz="0 0.07 -0.0578" rpy="1.5707963268 0 0" />
    <axis xyz="0 0 1" />
  </joint>
