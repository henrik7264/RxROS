<robot name="test_robot">
    <link name="base_link" />
    <link name="motor_link" />
    <link name="motor_link2" />
    <link name="touch_link" />
    <link name="touch_link2" />
    <link name="color_link" />
    <link name="range_link" />

    <joint name="joint1" type="continuous">
        <parent link="base_link"/>
        <child link="motor_link"/>
        <origin xyz="5 3 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>

    <joint name="joint2" type="continuous">
        <parent link="base_link"/>
        <child link="motor_link2"/>
        <origin xyz="-2 5 0" rpy="0 0 1.57" />
        <axis xyz="0 1 0" />
    </joint>

    <joint name="joint3" type="fixed">
        <parent link="base_link"/>
        <child link="touch_link"/>
        <origin xyz="5 0 0" rpy="0 0 -1.57" />
        <axis xyz="0 0 0" />
    </joint>

    <joint name="joint4" type="fixed">
        <parent link="base_link"/>
        <child link="touch_link2"/>
        <origin xyz="5 0 0" rpy="0 0 -1.57" />
        <axis xyz="0 0 0" />
    </joint>

    <joint name="joint5" type="fixed">
        <parent link="base_link"/>
        <child link="color_link"/>
        <origin xyz="5 0 0" rpy="0 0 -1.57" />
        <axis xyz="0 0 0" />
    </joint>

    <joint name="joint6" type="fixed">
        <parent link="base_link"/>
        <child link="range_link"/>
        <origin xyz="5 0 0" rpy="0 0 -1.57" />
        <axis xyz="0 0 0" />
    </joint>

</robot>
